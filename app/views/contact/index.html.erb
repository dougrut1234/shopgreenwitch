  <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<style>
.mySlides {display:none}
.w3-left, .w3-right, .w3-badge {cursor:pointer}
.w3-badge {height:13px;width:13px;padding:0}

#logo_background {
  height: 200px;
}

#contact_banner {
  width: 1200px;
  margin-top: -75px;
  margin-bottom: -100px;
  z-index: -100000;
}

#center_banner {
  text-align: center;
}


</style>

<body>



<!-- <p id="contact_title">CONTACT</p> -->

<p id="center_banner">
<%= image_tag("contact-background-contact", :id => "contact_banner") %>
</p>


<div  class="container2" style="z-index: 10000; position: relative; margin-top: -600px; margin-bottom: -150px;"">
  <form style="margin-top: 60px; z-index: 10000;" id="contact" action="" method="post" style="float: left; position: absolute;"">
    <h3 style="margin-top: 20px; z-index: 10000;">Talk To Us</h3>
    <h4>Our coven is commited to making your Green Witch experiences as magical as possible!</h4>
    <fieldset>
      <input placeholder="Your name" type="text" tabindex="1" required autofocus>
    </fieldset>
    <fieldset>
      <input placeholder="Your Email Address" type="email" tabindex="2" required>
    </fieldset>
    <fieldset>
      <input placeholder="Your Zip Code" type="zip" tabindex="3" required>
    </fieldset>
    <fieldset>
      <textarea placeholder="Type your Message Here." tabindex="5" required></textarea>
    </fieldset>
    <fieldset style="z-index: 1000;" id="submit-div">
      <button style="z-index: 10000;" name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
    </fieldset>
  </form>
</div>
<br>
<br>
<br>


</body>

